[user]
	name = Kirill Vinogradov
	email = vikian050194@gmail.com
[core]
	excludesfile = C:\\Users\\KirillV\\.gitignore_global
	preloadindex = true
	fscache = true
	autocrlf = true
	editor = vim
[color]
	ui = true
[alias]
	lg = log --graph --decorate --oneline
	la = log --graph --decorate --oneline --all
	lt = log --graph --decorate --oneline -10
	st = status -s
	ap = add -p
	ca = commit --amend
	cm = commit
	ch = checkout
	cb = "!f() { git checkout -b\"$1\"; }; f"
	cm = checkout master
	pu = "!f() { name=`git symbolic-ref HEAD --short`; git push --set-upstream origin $name; }; f"
	p = push
	pf = push -f
	mem = merge master
	fmem = "!f() { git fetch origin master && git merge origin/master; }; f"
	rem = rebase master
	frem = "!f() { git fetch origin master && git rebase origin/master; }; f"
	mt = mergetool
	dt = difftool
	cl = clean -f
	sl = stash list
	sd = stash drop stash@{0}
	rhh = reset --hard HEAD
	rh = reset --hard
	undo = reset --soft HEAD~
	dtp = diff HEAD~ HEAD
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[push]
	default = simple
[diff]
	tool = meld
	guitool = meld
[difftool]
	prompt = true
[merge]
	tool = meld
	guitool = meld
[mergetool]
	prompt = true
	keepBackup = false
[difftool "meld"]
	cmd = \"C:/Program Files (x86)/Meld/Meld.exe\" \"$LOCAL\" \"$REMOTE\"
	trustexistcode = true
	path = C:/Program Files (x86)/Meld/Meld.exe
[mergetool "meld"]
	cmd = \"C:/Program Files (x86)/Meld/Meld.exe\" \"$REMOTE\" \"$MERGED\" \"$LOCAL\"
	trustexistcode = true
	path = C:/Program Files (x86)/Meld/Meld.exe
[pull]
	rebase = false
[fetch]
	prune = false
[rebase]
	autoStash = false
